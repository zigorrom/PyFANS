            <COMBO selectedIndex="0" case_list="list[Vds,Vlg,Vbg]" name="function"/>
        </IN_CHANNEL>
    </NODE>
    <NODE name="out_settings">
        <OUT_CHANNEL name="och1">
            <CHECK name="enabled" checked="0"/>
            <COMBO selectedIndex="0" case_list="list[]" name="range"/>
            <COMBO selectedIndex="0" case_list="list[]" name="polarity"/>
            <COMBO selectedIndex="0" case_list="list[]" name="out_pin"/>
            <COMBO selectedIndex="0" case_list="list[]" name="function"/>
        </OUT_CHANNEL>
        <OUT_CHANNEL name="och2">
            <CHECK name="enabled" checked="0"/>
            <COMBO selectedIndex="0" case_list="list[]" name="range"/>
            <COMBO selectedIndex="0" case_list="list[]" name="polarity"/>
            <COMBO selectedIndex="0" case_list="list[]" name="out_pin"/>
            <COMBO selectedIndex="0" case_list="list[]" name="function"/>
        </OUT_CHANNEL>
