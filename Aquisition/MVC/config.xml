<NODE name="Settings">
    <ACQUISITION_SETTINGS name="acquisition_settings">
        <NUMERIC name="sample_rate" value="10"/>
        <NUMERIC name="points_per_shot" value="10"/>
        <CHECK name="homemade_amplifier" checked="0"/>
        <COMBO selectedIndex="0" case_list="list[]" name="pga_gain"/>
        <COMBO selectedIndex="0" case_list="list[]" name="filter_gain"/>
        <COMBO selectedIndex="0" case_list="list[]" name="filter_cutoff"/>
    </ACQUISITION_SETTINGS>
    <NODE name="input_settings">
        <IN_CHANNEL name="ch1">
            <CHECK name="enabled" checked="0"/>
            <COMBO selectedIndex="0" case_list="list[One,Two,Three]" name="range"/>
            <COMBO selectedIndex="0" case_list="list[Unipolar,Bipolar]" name="polarity"/>
            <COMBO selectedIndex="0" case_list="list[Vds,Vlg,Vbg]" name="function"/>
        </IN_CHANNEL>
        <IN_CHANNEL name="ch2">
